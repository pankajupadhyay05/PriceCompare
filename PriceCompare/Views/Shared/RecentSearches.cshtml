@model IEnumerable<PriceCompare.Models.Term>
           
<div id="leftList">
    <div class="leftListTitle">
        <h3>Popular Searches</h3>
    </div>
    <div class="leftListItem">
        <ul>
            @foreach (var item in Model)
            {
                <li>@Html.ActionLink(item.Query, "Search", "Home", new { query = item.Query }, null)</li>
            }
        </ul>
    </div>
</div>
            